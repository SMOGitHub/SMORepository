// Seção de Declarações 
var
   exercicioEscolhido, tituloEscolhido, tituloResultado, tituloRetornando: caractere
   anoAtual: inteiro
   
   //Exercício 1
   procedimento Exercicio1()
   var
   inicio
      escreval("Olá, Mundo!")
   fimprocedimento

   //Exercício 2
   procedimento Exercicio2()
   var
      nome: caractere
   inicio
      escreva("Qual é o seu nome? ")
      leia(nome)
      escreval("Olá ", nome, ", é um prazer te conhecer!")
   fimprocedimento

   //Exercício 3
   procedimento Exercicio3()
   var
      nome: caractere
      salario: real
   inicio
      escreva("Nome do Funcionário: ")
      leia(nome)
      escreva("Salário: ")
      leia(salario)
      escreval("O funcionário ", nome, " tem um salário de R$", salario:1:2, " em Junho.")
   fimprocedimento
   
  //Exercício 4
   procedimento Exercicio4()
   var
      num1, num2: inteiro
   inicio
      escreva("Digite um valor: ")
      leia(num1)
      escreva("Digite outro valor: ")
      leia(num2)
      escreval("A soma entre", num1," e", num2, " é igual a", num1 + num2, ".")
   fimprocedimento
   
   //Exercício 5
   procedimento Exercicio5()
   var
      nota1, nota2, media: real
   inicio
      escreva("Nota 1: ")
      leia(nota1)
      escreva("Nota 2: ")
      leia(nota2)
      media <- (nota1 + nota2) / 2
      escreval("A média entre ", nota1:2:1, " e ", nota2:2:1, " é igual a ", media:2:1)
   fimprocedimento
   
   //Exercício 6
   procedimento Exercicio6()
   var
      num: inteiro
   inicio
      escreva("Digite um número: ")
      leia(num)
      escreval("O antecessor de", num, " é", num-1)
      escreval("O sucessor de", num, " é", num+1)
   fimprocedimento
   
   //Exercício 7
   procedimento Exercicio7()
   var
      num: real
   inicio
      escreva("Digite um número: ")
      leia(num)
      escreval("O dobro de ", num, " é ", 2 * num:2:1)
      escreval("A terça parte de ", num, " é ", 1/3 * num:2:5)
   fimprocedimento
   
   //Exercício 8
   procedimento Exercicio8()
   var
      //As variáveis "comprimento" e "espacos" são usadas para calcular a
      //distância correta entre uma coluna e outra e não são necessárias
      //para a realização do exercício
      distancia, comprimento: real
      espacos: caractere
   inicio
      espacos <- "                    "
      escreva("Digite uma distância em metros: ")
      leia(distancia)
      escreval("A distância de ", distancia:2:1, "m corresponde a:")
      comprimento <- compr(numpcarac(1 / 1000 * distancia))
      escreva(1 / 1000 * distancia, "Km")
      escreval(copia(espacos, 1, 20 - comprimento), 10 * distancia, "dm")
      comprimento <- compr(numpcarac(1 / 100 * distancia))
      escreva(1 / 100 * distancia, "Hm")
      escreval(copia(espacos, 1, 20 - comprimento), 100 * distancia, "cm")
      comprimento <- compr(numpcarac(1 / 10 * distancia))
      escreva(1 / 10 * distancia, "Dam")
      escreval(copia(espacos, 1, 19 - comprimento), 1000 * distancia, "mm")
   fimprocedimento
   
   //Exercício 9
   procedimento Exercicio9()
   var
      dinheiro: real
   inicio
      escreva("Digite o valor do seu salário: R$")
      leia(dinheiro)
      escreval("Seu salário vale, em dólares: US$", 1 / 3.45 * dinheiro:3:2)
   fimprocedimento
   
   //Exercício 10
   procedimento Exercicio10()
   var
      largura, altura, area: real
   inicio
      escreva("Escreva a largura da parede em metros: ")
      leia(largura)
      escreva("Escreva a altura da parede em metros: ")
      leia(altura)
      area <- largura * altura
      escreval("A área da parede é de ", area:2:1, " metros quadrados.")
      escreval("A quantidade de tinta necessária para pintar essa parede é de ", 1/2 * area:2:1, "L")
   fimprocedimento
   
   //Exercício 11
   procedimento Exercicio11()
   var
      valorA, valorB, valorC, delta: real
   inicio
      escreva("Digite o valor de A: ")
      leia(valorA)
      escreva("Digite o valor de B: ")
      leia(valorB)
      escreva("Digite o valor de C: ")
      leia(valorC)
      delta <- valorB^2 - 4 * valorA * valorC
      escreval("O valor de Delta para esta equação é:", delta)
   fimprocedimento

   //Exercício 12
   procedimento Exercicio12()
   var
      preco: real
   inicio
      escreva("Digite o preço do produto: R$")
      leia(preco)
      escreval("O valor do produto com 5% de desconto é: R$", 0.95 * preco:3:2)
   fimprocedimento
   
   //Exercício 13
   procedimento Exercicio13()
   var
      preco: real
   inicio
      escreva("Digite o salário do funcionário: R$")
      leia(preco)
      escreval("O salário desse funcionário com 15% de aumento é: R$", 1.15 * preco:3:2)
   fimprocedimento
   
   //Exercício 14
   procedimento Exercicio14()
   var
      kilometros, dias: real
   inicio
      escreva("Quantos KM foram percorridos pelo carro? ")
      leia(kilometros)
      escreva("Por quantos dias o carro foi alugado? ")
      leia(dias)
      escreval("O valor a ser cobrado do cliente é de: R$", (90 * dias  + 0.2 * kilometros):3:2)
   fimprocedimento
   
   //Exercício 15
   procedimento Exercicio15()
   var
      dias: real
   inicio
      escreva("Dias de trabalho: ")
      leia(dias)
      escreval("Salário do funcionário: R$", 25 * 8 * dias:3:2)
   fimprocedimento
   
   //Exercício 16
   procedimento Exercicio16()
   var
      cigarros, anos: real
   inicio
      escreva("Quantos cigarros são fumados por dia? ")
      leia(cigarros)
      escreva("A quantos é um fumante? ")
      leia(anos)
      escreval("O tempo de vida perdido por fumar é de ", (anos * 365 * cigarros * 10 / (60 * 24)):2:1, " dias")
   fimprocedimento
   
   //Exercício 17
   procedimento Exercicio17()
   var
      velocidade: real
   inicio
      escreva("Digite a velocidade do carro (em km/h): ")
      leia(velocidade)
      se (velocidade > 80) entao
         escreval("O motorista deve ser multado em R$", ((velocidade - 80)*5):3:2)
      senao
         escreval("O motorista está dentro do limite permitido de velocidade.")
      fimse
   fimprocedimento
   
   //Exercício 18
   procedimento Exercicio18()
   var
      ano: real
   inicio
      escreva("Digite o ano de nascimento do usuário: ")
      leia(ano)
      se (anoAtual - ano >= 16) entao
         escreval("O usuário já pode votar.")
      senao
         escreval("O usuário ainda não pode votar.")
      fimse
   fimprocedimento
   
   //Exercício 19
   procedimento Exercicio19()
   var
      nome: caractere
      nota1, nota2, media: real
   inicio
      escreva("Nome do aluno: ")
      leia(nome)
      escreva("Nota 1 (0-10): ")
      leia(nota1)
      escreva("Nota 2 (0-10): ")
      leia(nota2)
      media <- (nota1 + nota2)/2
      escreval("Média: ", media:2:1)
      se (media >= 7) entao
         escreval("O aluno ", nome, " foi aprovado.")
      senao
         escreval("O aluno ", nome, " foi reprovado.")
      fimse
   fimprocedimento
   
   //Exercício 20
   procedimento Exercicio20()
   var
      num: inteiro
   inicio
      escreva("Digite um número inteiro: ")
      leia(num)
      se (num % 2 = 0) entao
         escreval("O número", num," é par.")
      senao
         escreval("O número", num," é ímpar.")
      fimse
   fimprocedimento
   
   //Exercício 21
   procedimento Exercicio21()
   var
      ano: inteiro
   inicio
      escreva("Escreva um ano: ")
      leia(ano)
      se (ano % 4 = 0) entao
         escreval("O ano", ano," é bissexto.")
      senao
         escreval("O ano", ano," NÃO é bissexto.")
      fimse
   fimprocedimento
   
   //Exercício 22
   procedimento Exercicio22()
   var
      ano, idade: inteiro
   inicio
      escreva("Escreva o ano de nascimento: ")
      leia(ano)
      idade <- anoAtual - ano
      se (idade >= 18) entao
         escreval("O alistamento já deveria ter sido feito a", idade - 18," ano(s).")
      senao
         escreval("Ainda faltam", 18 - idade," ano(s) para o alistamento.")
      fimse
   fimprocedimento
   
   //Exercício 23
   procedimento Exercicio23()
   var
      nome, genero: caractere
      compras: real
   inicio
      escreva("Digite o nome do cliente: ")
      leia(nome)
      escreva("Digite o gênero do cliente (M -> Masculino / F - Feminino): ")
      leia(genero)
      escreva("Digite o valor em compras: R$")
      leia(compras)
      se (maiusc(genero) = "M") entao
         escreval("O valor cobrado do ", nome," será de R$", 0.95*compras:3:2, ".")
      senao
         escreval("O valor cobrado da ", nome," será de R$", 0.87*compras:3:2, ".")
      fimse
   fimprocedimento
   
   //Exercício 24
   procedimento Exercicio24()
   var
      distancia: real
   inicio
      escreva("Quantos quilômetros desejas percorrer? ")
      leia(distancia)
      se (distancia > 200) entao
         escreval("O valor a ser cobrado será de R$", 0.45*distancia:3:2)
      senao
         escreval("O valor a ser cobrado será de R$", 0.50*distancia:3:2)
      fimse
   fimprocedimento
   
   //Exercício 25
   procedimento Exercicio25()
   var
      lado1, lado2, lado3: real
   inicio
      escreva("Digite o valor primeiro lado: ")
      leia(lado1)
      escreva("Digite o valor segundo lado: ")
      leia(lado2)
      escreva("Digite o valor terceiro lado: ")
      leia(lado3)
      se (lado1 < (lado2 + lado3)) e (lado2 < (lado1 + lado3)) e (lado3 < (lado1 + lado2)) entao
         escreval("Um triângulo pode ser formado com lados de comprimento", lado1, ", ", lado2, " e ", lado3)
      senao
         escreval("Não é possível formar um triângulo com lados de comprimento", lado1, ",", lado2, " e", lado3)
      fimse
   fimprocedimento

   //Exercício 26
   procedimento Exercicio26()
   var
      valor1, valor2: real
   inicio
      escreva("Digite o primeiro número inteiro: ")
      leia(valor1)
      escreva("Digite segundo número inteiro: ")
      leia(valor2)
      se (valor1 > valor2) entao
         escreval("O primeiro valor é o maior.")
      senao
           se (valor2 > valor1) entao
              escreval("O segundo valor é o maior.")
           senao
              escreval("Não existe valor maior, os dois valores são iguais.")
           fimse
      fimse
   fimprocedimento
   
   //Exercício 27
   procedimento Exercicio27()
   var
      nota1, nota2, media: real
   inicio
      escreva("Digite a primeira nota (0-10): ")
      leia(nota1)
      escreva("Digite a segunda nota(0-10): ")
      leia(nota2)
      media <- (nota1 + nota2) / 2
      se (media < 5) entao
         escreval("A média desse aluno é ", media:2:1, ". Status: REPROVADO")
      senao
           se (media < 7) entao
              escreval("A média desse aluno é ", media:2:1, ". Status: RECUPERAÇÃO")
           senao
              escreval("A média desse aluno é ", media:2:1, ". Status: APROVADO")
           fimse
      fimse
   fimprocedimento
   
   //Exercício 28
   procedimento Exercicio28()
   var
      largura, comprimento, area: real
   inicio
      escreva("Digite a largura do terreno (em metros): ")
      leia(largura)
      escreva("Digite o comprimento do terreno (em metros): ")
      leia(comprimento)
      area <- largura * comprimento
      se (area < 100) entao
         escreval("A área desse terreno é ", area:2:1, " m^2 e ele está classificado como: TERRENO POPULAR")
      senao
           se (area <= 500) entao
              escreval("A área desse terreno é ", area:2:1, " m^2 e ele está classificado como: TERRENO MASTER")
           senao
              escreval("A área desse terreno é ", area:2:1, " m^2 e ele está classificado como: TERRENO VIP")
           fimse
      fimse
   fimprocedimento
   
   //Exercício 29
   procedimento Exercicio29()
   var
      nome: caractere
      salario, anos: real
   inicio
      escreva("Digite o nome do funcionário: ")
      leia(nome)
      escreva("Digite o salário atual do funcionário: R$")
      leia(salario)
      escreva("Digite a quantos anos este funcionário está nesta empresa: ")
      leia(anos)
      se (anos <= 3) entao
         escreval("O novo salário do(a) ", nome, " é de R$", 1.03*salario:3:2)
      senao
           se (anos < 10) entao
              escreval("O novo salário do(a) ", nome, " é de R$", 1.125*salario:3:2)
           senao
              escreval("O novo salário do(a) ", nome, " é de R$", 1.20*salario:3:2)
           fimse
      fimse
   fimprocedimento
   
   //Exercício 30
   procedimento Exercicio30()
   var
      lado1, lado2, lado3: real
   inicio
      escreva("Digite o valor primeiro lado: ")
      leia(lado1)
      escreva("Digite o valor segundo lado: ")
      leia(lado2)
      escreva("Digite o valor terceiro lado: ")
      leia(lado3)
      se (lado1 < (lado2 + lado3)) e (lado2 < (lado1 + lado3)) e (lado3 < (lado1 + lado2)) entao
         se ((lado1 = lado2) e (lado2 = lado3)) entao
            escreval("Um triângulo equilátero é formado.")
         senao
              se (lado1 = lado2) ou (lado2 = lado3) ou (lado3 = lado1) entao
                 escreval("Um triângulo isósceles é formado.")
              senao
                 escreval("Um triângulo escaleno é formado.")
              fimse
         fimse
      senao
         escreval("Não é possível formar um triângulo com lados de comprimento", lado1, ", ", lado2, " e ", lado3)
      fimse
   fimprocedimento
   
   //Exercício 31
   procedimento Exercicio31()
   var
      ferramenta, maquina: inteiro
      ferramentas: vetor[1..3] de caractere
   inicio
      ferramentas[1] <- "Tesoura"
      ferramentas[2] <- "Pedra"
      Ferramentas[3] <- "Papel"
      escreval("---JoKemPo---")
      escreval("1 - Tesoura")
      escreval("2 - Pedra")
      escreval("3 - Papel")
      escreva("Escolha uma ferramenta: ")
      leia(ferramenta)
      enquanto (ferramenta < 1) ou (ferramenta > 3) faca
          escreval("---JoKemPo---")
          escreval("1 - Tesoura")
          escreval("2 - Pedra")
          escreval("3 - Papel")
          escreva("Valor inválido! Escolha uma ferramenta: ")
          leia(ferramenta)
      fimenquanto
      maquina <- randi(3) + 1
      escreval()
      escreval("<<FIGHT>> Jogador com ", ferramentas[ferramenta], " VS Máquina com ", ferramentas[maquina], " <<FIGHT>>")
      escreval()
      se (ferramenta = maquina) entao
         escreval("Empate...")
      senao
           se (ferramenta = 1) entao
              se (maquina = 2) entao
                 escreval("Máquina vence: Pedra quebra Tesoura.")
              senao
                 escreval("Jogador vence: Tesoura corta Papel.")
              fimse
           senao
              se (ferramenta = 2) entao
                 se (maquina = 1) entao
                    escreval("Jogador vence: Pedra quebra Tesoura.")
                 senao
                    escreval("Máquina vence: Papel enrola Pedra.")
                 fimse
              senao
                   se (maquina = 1) entao
                      escreval("Máquina vence: Tesoura corta Papel.")
                   senao
                      escreval("Jogador vence: Papel enrola Pedra.")
                   fimse
              fimse
           fimse
      fimse
   fimprocedimento

   //Exercício 32
   procedimento Exercicio32()
   var
      valorAleatorio, chute: inteiro
   inicio
      valorAleatorio <- randi(5) + 1
      escreva("Em que número estou pensando? [1..5] ")
      leia(chute)
      se (chute = valorAleatorio) entao
         escreva("Parabéns, você acertou! O número que pensei é", chute, ".")
      senao
         escreva("Haha, você errou! Eu pensei no número", valorAleatorio, ".")
      fimse
      escreval()
   fimprocedimento
   
   //Exercício 33
   procedimento Exercicio33()
   var
      valorCasa, salario, anosDivida, mensalidade: real
   inicio
      escreva("Digite o valor da casa: ")
      leia(valorCasa)
      escreva("Salário do comprador: ")
      leia(salario)
      escreva("Em quantos anos o comprador deseja pagar? ")
      leia(anosDivida)
      enquanto (anosDivida = 0) faca
            escreva("Digite o valor da casa:", valorCasa)
            escreva("Salário do comprador:", salario)
            escreva("Valor inválido! Em quantos anos o comprador deseja pagar? ")
            leia(anosDivida)
      fimenquanto
      mensalidade <- valorCasa / (12 * anosDivida)
      se (mensalidade > 0.3 * salario) entao
         escreval("Empréstimo negado, o valor da mensalidade excede 30% do salário do comprador.")
      senao
         escreval("Empréstimo aprovado!")
      fimse
   fimprocedimento
   
   //Exercício 34
   procedimento Exercicio34()
   var
      massa, altura, imc: real
   inicio
      escreva("Digite o peso da pessoa (em kg): ")
      leia(massa)
      escreva("Digite a altura dessa pessoa (em metros): ")
      leia(altura)
      enquanto (altura = 0) faca
         escreva("Digite o peso da pessoa (em kg):", massa)
         escreva("Altura inválida. Digite a altura dessa pessoa (em metros): ")
         leia(altura)
      fimenquanto
      imc <- massa/altura^2
      se (imc < 18.5) entao
         escreval("Essa pessoa está abaixo do peso ideal.")
      senao
         se (imc < 25) entao
            escreval("Essa pessoa está no peso ideal.")
         senao
            se (imc < 30) entao
               escreval("Essa pessoa está com sobrepeso.")
            senao
               se (imc <= 40) entao
                  escreval("Essa pessoa está obesa.")
               senao
                  escreval("Essa pessoa está com obesidade mórbida.")
               fimse
            fimse
         fimse
      fimse
   fimprocedimento
   
   //Exercício 35
   procedimento Exercicio35()
   var
      tipoDeCarro: inteiro
      dias, kilometros: real
   inicio
      escreva("Digite o tipo de carro escolhido (1-Luxo / 2-Comum): ")
      leia(tipoDeCarro)
      escreva("Por quantos dias o carro foi alugado? ")
      leia(dias)
      escreva("Quantos kilômetros o carro rodou? ")
      leia(kilometros)
      se (tipoDeCarro = 1) entao
         se (kilometros > 200) entao
            escreval("O valor a ser pago é de R$", 150 * dias + 0.25 * kilometros:3:2)
         senao
            escreval("O valor a ser pago é de R$", 150 * dias + 0.30 * kilometros:3:2)
         fimse
      senao
         se (kilometros > 100) entao
            escreval("O valor a ser pago é de R$", 90 * dias + 0.1 * kilometros:3:2)
         senao
            escreval("O valor a ser pago é de R$", 90 * dias + 0.2 * kilometros:3:2)
         fimse
      fimse
   fimprocedimento
   
   //Exercício 36
   procedimento Exercicio36()
   var
      horas, pontos: real
   inicio
      escreva("Quantas horas de atividade essa pessoa teve no mês? ")
      leia(horas)
      se (horas < 10) entao
         pontos <- horas * 2
      senao
         se (horas <= 20) entao
            pontos <- horas * 5
         senao
            pontos <- horas * 10
         fimse
      fimse
      escreval("Essa pessoa acumulou", pontos, " pontos e ganhará R$", 0.05 * pontos:3:2)
   fimprocedimento
   
   //Exercício 37
   procedimento Exercicio37()
   var
      salario, genero, anos: real
   inicio
      escreva("Qual o salário atual desse funcionário? R$")
      leia(salario)
      escreva("Qual o gênero desse funcionnário? (1 - Masculino / 2 - Feminino) ")
      leia(genero)
      escreva("A quantos anos esse funcionário trabalha nessa empresa? ")
      leia(anos)
      se (genero = 1) entao
         se (anos < 20) entao
            escreval("O novo salário desse funcionário é R$", 1.03 * salario:3:2)
         senao
            se (anos <= 30) entao
               escreval("O novo salário desse funcionário é R$", 1.13 * salario:3:2)
            senao
               escreval("O novo salário desse funcionário é R$", 1.25 * salario:3:2)
            fimse
         fimse
      senao
         se (anos < 15) entao
            escreval("O novo salário desse funcionário é R$", 1.05 * salario:3:2)
         senao
            se (anos <= 20) entao
               escreval("O novo salário desse funcionário é R$", 1.12 * salario:3:2)
            senao
               escreval("O novo salário desse funcionário é R$", 1.23 * salario:3:2)
            fimse
         fimse
      fimse
   fimprocedimento
   
   //Exercicio 38
   procedimento Exercicio38()
   var
      num: inteiro
   inicio
         para num de 6 ate 11 faca
              escreva(num)
         fimpara
         escreva(" Acabou!")
   fimprocedimento
   
   //Exercicio 39
   procedimento Exercicio39()
   var
      num: inteiro
   inicio
         num <- 10
         enquanto (num > 2) faca
              escreva(num)
              num <- num - 1
         fimenquanto
         escreva(" Acabou!")
   fimprocedimento
   
   //Exercicio 40
   procedimento Exercicio40()
   var
      num: inteiro
   inicio
         para num de 0 ate 18 passo 3 faca
              escreva(num)
         fimpara
         escreva(" Acabou!")
   fimprocedimento
   
   //Exercicio 41
   procedimento Exercicio41()
   var
      num: inteiro
   inicio
         para num de 100 ate 0 passo -5 faca
              escreva(num)
         fimpara
         escreva(" Acabou!")
   fimprocedimento
   
   //Exercicio 42
   procedimento Exercicio42()
   var
      num, n: inteiro
   inicio
         num <- 0
         escreva("Digite um valor: ")
         leia(num)
         enquanto (num < 1) faca
            escreva("Número inválido! Digite um número maior que zero: ")
            leia(num)
         fimenquanto
         para n de 1 ate num faca
              escreva(n)
         fimpara
         escreva(" Acabou!")
   fimprocedimento
   
   //Exercicio 43
   procedimento Exercicio43()
   var
      num: inteiro
   inicio
         para num de 30 ate 1 passo -1 faca
              se (num % 4 = 0) entao
                 escreva(" [", numpcarac(num), "]")
              senao
                 escreva(num)
              fimse
         fimpara
         escreva(" Acabou!")
   fimprocedimento

   //Exercicio 44
   procedimento Exercicio44()
   var
      comeco, final, incremento, n: inteiro
      erro: logico
   inicio
         erro <- FALSO
         escreva("Digite o primeiro Valor: ")
         leia(comeco)
         escreva("Digite o último Valor: ")
         leia(final)
         escreva("Digite o incremento: ")
         leia(incremento)
         se (comeco > final) entao
            se (incremento > 0) ou (incremento = 0) entao
               erro <- VERDADEIRO
            fimse
         senao
            se (comeco < final) entao
               se (incremento < 0) ou (incremento = 0) entao
                  erro <- VERDADEIRO
               fimse
            fimse
         fimse
         se (erro) entao
            escreval("Os valores digitados formarão um loop infinito!")
         senao
            para n de comeco ate final passo incremento faca
              escreva(n)
            fimpara
            escreva(" Acabou!")
         fimse
   fimprocedimento
   
   //Exercicio 45
   procedimento Exercicio45()
   var
      comeco, final, incremento, i, n: inteiro
      erro: logico
   inicio
         erro <- FALSO
         escreva("Digite o primeiro Valor: ")
         leia(comeco)
         escreva("Digite o último Valor: ")
         leia(final)
         escreva("Digite o incremento: ")
         leia(incremento)
         se (comeco > final) entao
            se (incremento > 0) entao
               i <- comeco
               comeco <- final
               final <- i
               escreval("Inicio e final invertidos para prevenir loop infinito.")
            fimse
         senao
            se (comeco < final) entao
               se (incremento < 0) entao
                  i <- comeco
                  comeco <- final
                  final <- i
                  escreval("Inicio e final invertidos para prevenir loop infinito.")
               fimse
            fimse
         fimse
         se (incremento = 0) e (comeco <> final) entao
            erro <- VERDADEIRO
         fimse
         se erro entao
            escreval("Os valores digitados formarão um loop infinito!")
         senao
            para n de comeco ate final passo incremento faca
              escreva(n)
            fimpara
            escreva(" Acabou!")
         fimse
   fimprocedimento
   
   //Exercício 46
   procedimento Exercicio46()
   var
      soma, n: inteiro
   inicio
         soma <- 0
         para n de 6 ate 100 passo 2 faca
              soma <- soma + n
         fimpara
         escreval("A soma dos números no intervalo [6..100] é", soma)
   fimprocedimento
   
