   //Exercício 48
   procedimento Exercicio48()
   var
      numero, soma, n: inteiro
   inicio
         soma <- 0
         para n de 1 ate 7 faca
              escreva("Digite um número inteiro: ")
              leia(numero)
              soma <- soma + numero
         fimpara
         escreval("A soma dos números digitados é", soma)
   fimprocedimento
   
   //Exercício 49
   procedimento Exercicio49()
   var
      numero, pares, n: inteiro
   inicio
         pares <- 0
         para n de 1 ate 6 faca
              escreva("Digite um número inteiro: ")
              leia(numero)
              se (numero % 2 = 0) entao
                 pares <- pares + 1
              fimse
         fimpara
         escreval("Números pares:", pares, " | Números ímpares:", 6 - pares)
   fimprocedimento
   
   //Exercício 50
   procedimento Exercicio50()
   var
      numeros: vetor[1..20] de inteiro
      maiorQ5, divisivelP3, n: inteiro
   inicio
         maiorQ5 <- 0
         divisivelP3 <- 0
         escreva("Os números escolhidos foram:")
         para n de 1 ate 20 faca
              numeros[n] <- randi(10) + 1
              se (numeros[n] > 5) entao
                 maiorQ5 <- maiorQ5 + 1
              fimse
              se (numeros[n] % 3 = 0) entao
                 divisivelP3 <- divisivelP3 + 1
              fimse
              escreva(numeros[n])
         fimpara
         escreval()
         escreval("Números acima de 5:", maiorQ5)
         escreval("Números divisíveis por 3:", divisivelP3)
   fimprocedimento
   
   //Usando uma maneira mais bonita para pular linhas
   procedimento PularLinha()
   var
   inicio
         escreval("")
   fimprocedimento
   
   //Escrevendo título antes do resultado
   procedimento EscrevaTituloResultado():
   var
   inicio
         PularLinha()
         escreval(tituloResultado)
   fimprocedimento

   //Escrevendo título antes de voltar ao programa principal
   procedimento EscrevaTituloRetornando():
   var
      resposta: caractere
   inicio
         PularLinha()
         escreva("Aperte ENTER para continuar.")
         leia(resposta)
         escreva(tituloRetornando)
         LimpaTela
   fimprocedimento

